    var si;
    function samePage() {
        return document.applications.newLocation.selectedIndex != 0;
    }

    function jumpPage(newLoc) {
        newPage = newLoc.options[newLoc.selectedIndex].value;

        if (newPage != "") {
            window.location.href = newPage
        }

    }

    function newWindow(fileName) {
        window.open(fileName, "win", "width=400,height=600,scrollbars=yes");
    }
</script>
