    <script language="javascript">
        function hasPermission() {
            if (document.frm.PERMISSION != null && (document.frm.PERMISSION.value == "false")) {

                document.frm.responseoption[0].checked = true;
                alert("You are not authorized for the Historical Tracing and your response option is being changed to Last Event.");
                document.frm.PERMISSION.value = "true";
            }
        }

        function enableHistoryDateRange() {
            hasPermission();
            if (document.frm.responseoption != null) {
                if (document.frm.responseoption[2] != null && document.frm.responseoption[2].checked) {
                    setDisableHistoryDateRange(false);
                }
            }
        }
        function setDisableHistoryDateRange(bool) {
            if (document.frm.historicalDateRange != null)
                document.frm.historicalDateRange.disabled = bool;
        }

        function showtip(current, e, text) {

            if (document.all) {
                thetitle = text.split('<br />');
                if (thetitle.length > 1) {
                    thetitles = "";
                    for (i = 0; i < thetitle.length; i++)
                        thetitles += thetitle[i] + "\r\n"
                    current.title = thetitles
                }
                else current.title = text
            }

            else if (document.layers) {
                document.tooltip.document.write(
                    '<layer bgColor="#FFFFE7" style="border:1px ' +
                    'solid black; font-size:12px;color:#000000;">' + text + '</layer>');
                document.tooltip.document.close();
                document.tooltip.left = e.pageX + 5;
                document.tooltip.top = e.pageY - 75;
                document.tooltip.visibility = "show"
            }
        }

        function hidetip() {

            if (document.layers)
                document.tooltip.visibility = "hidden"
        }


        <!-- Written by Ujjal Borua for Gnats #1387 0n 13-05-2002 -->

        var splitIndex = 0;
        var splitArray = [];

        function splits(string, text) {
            var strLength = string.length, txtLength = text.length;
            if ((strLength == 0) || (txtLength == 0)) return;

            var i = string.indexOf(text);
            if ((!i) && (text != string.substring(0, txtLength))) return;
            if (i == -1) {
                splitArray[splitIndex++] = string;
                return;
            }

            splitArray[splitIndex++] = string.substring(0, i);

            if (i + txtLength < strLength)
                splits(string.substring(i + txtLength, strLength), text);


        }

        function split(string, text) {
            splitIndex = 0;
            splits(string, text);
        }

        function join(arrayName) {
            var temp = '';
            for (var i = 0; i < splitIndex; i++)
                temp += arrayName[i] + '\n';
            if (escape(temp.substring(0, 1)) == '%0A')
                return temp.substring(1, temp.length);
            else
                return temp;
        }

        function sort(arrayName) {
            for (var i = 0; i < (splitIndex - 1); i++)
                for (var j = i + 1; j < splitIndex; j++)
                    if (arrayName[j].toLowerCase() < arrayName[i].toLowerCase()) {
                        var dummy = arrayName[i];
                        arrayName[i] = arrayName[j];
                        arrayName[j] = dummy;
                    }
        }

        function ltrim(s) {
            return s.replace(/^\s*/, "")
        }

        function rtrim(s) {
            return s.replace(/\s*$/, "");
        }

        function split(string, text) {
            splitArray = string.split(text);
            splitIndex = splitArray.length;
        }

        function replace(string, text, by) {
            // Replaces text with by in string
            var strLength = string.length, txtLength = text.length;
            if ((strLength == 0) || (txtLength == 0)) return string;

            var i = string.indexOf(text);
            if ((!i) && (text != string.substring(0, txtLength))) return string;
            if (i == -1) return string;

            var newstr = string.substring(0, i) + by;

            if (i + txtLength < strLength)
                newstr += replace(string.substring(i + txtLength, strLength), text, by);


            return newstr;

        }

        function sorttext() {
            document.frm.carlist.value = replace(document.frm.carlist.value, ',', '\n');
            document.frm.carlist.value = replace(document.frm.carlist.value, ' ', '');
            split(document.frm.carlist.value, '\n');
            sort(splitArray);
            document.frm.carlist.value = rtrim(ltrim(join(splitArray)));
        }

        function checkForSortOrderDups() {
            // Note: We may want a script in the future that eliminates choices that are in the lists once the first choice
            // is made - This would avoid having to alert the user. We just wouldn't give them the option of selecting dups

            // 1st check - If 'NONE' is selected for 2nd and 3rd we move on - this dup is allowed
            if (document.frm.sort_order_2.options[document.frm.sort_order_2.selectedIndex].text == "None") {
                if (document.frm.sort_order_3.options[document.frm.sort_order_3.selectedIndex].text == "None") {
                    return true;
                }
                else {
                    // If 'NONE' is the 2nd option and a 3rd is not chosen, return false;
                    return false;
                }
            }

            // Get the selected sort orders (For some reason I can't substitute these vars for the above if clause - doesn't work in Netscape 4.5...
            var sortOrder1 = document.frm.sort_order_1.options[document.frm.sort_order_1.selectedIndex].text;
            var sortOrder2 = document.frm.sort_order_2.options[document.frm.sort_order_2.selectedIndex].text;
            var sortOrder3 = document.frm.sort_order_3.options[document.frm.sort_order_3.selectedIndex].text;

            // 2nd check -  Now check for other dups
            if (sortOrder1 == sortOrder2 || sortOrder2 == sortOrder3 || sortOrder1 == sortOrder3) {
                return false;
            }

            return true;
        }

        <!--SET 072601 - Added check for sort order as well as sort order function above; Added translation tag for Vin alert-->
        function validateForm() {
            for (i = 0; i < document.frm.elements.length; i++) {
                // Test for nulls, empty fields
                if (document.frm.elements[i].value == "" || document.frm.elements[i].value == null || document.frm.elements[i].value.length < 1) {
                    // Check cars
                    if (document.frm.elements[i].name == "carlist") {
                        alert("Please enter Car numbers before searching.");
                        document.frm.elements[i].focus();
                        return false;
                    }
                }

                // Check specific sort orders - can't have duplicates here
                if (document.frm.elements[i].name == "sort_order_1") {
                    if (!checkForSortOrderDups()) {
                        alert("Sort Order selections are not valid. Please go back and select distinct sorting criteria. Note that 2nd sort criteria cannot be set to NONE if 3rd sort criteria is selected.");
                        document.frm.elements[i].focus();
                        return false;
                    }

                }
            }
            if (document.frm.responseoption[2] != null) {
                if (document.frm.responseoption[2].checked) {
                    if (document.frm.historicalDateRange.options[document.frm.historicalDateRange.selectedIndex].text == "Select Historical Days") {
                        alert("Please select the number of days for tracing events historically.");
                        document.frm.historicalDateRange.focus();
                        return false;
                    }
                }
            }
            return true;
        }


        function getController(submitValue) {
            document.frm.target = "_self";
            //suppress display of pop up window if validation fails
            if (validateForm()) {
                if (submitValue == "RUNTRACE") {
                    document.frm.CONTROLLER.value = "ETPrepareTraceToRunCtlr";
                    var winURL="/AttrTraceReportProcessingTemplate.jsp";
                    var winName = "traceWindow";
                    var winSpecs = 'height=590,width=790,scrollbars=yes,menubar=yes,resizable=yes,left=0,top=0';
                    win1 = window.open(winURL, winName, winSpecs);
                }
                else if (submitValue == "SAVETRACE") {
                    document.frm.CONTROLLER.value = "ETPrepareTraceForSaveCtlr";
                }
                else if (submitValue == "SAVETRACEAS") {
                    document.frm.CONTROLLER.value = "ETPrepareTraceForSaveAsCtlr";
                }
            } else {
                return false;
            }
        }

    </script>

</head>

