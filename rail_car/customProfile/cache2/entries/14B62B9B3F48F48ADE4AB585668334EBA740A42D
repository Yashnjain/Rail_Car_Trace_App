<script language="JavaScript" type="text/javascript">

  function setItUp(){
        if (! (typeof(NOMARGIN) == "undefined")) {
		document.getElementById('appArea').style.marginLeft = "0px";
		document.getElementById('footerContent').style.marginLeft = "100px";
		return;
	}

     if ( document.getElementById( 'stLeftNavLook' ) ){
			document.getElementById( 'leftNavLook' ).style.marginRight = "20px";
			document.getElementById('footerContent').style.marginLeft = "150px";
     } else if(document.getElementById('leftNavLook') || document.getElementById('siNavLook')){
			document.getElementById('appArea').style.marginLeft = "100px";
			document.getElementById('footerContent').style.marginLeft = "150px";
	 } else if(document.getElementById('siPatternNavLook')){
			document.getElementById('appArea').style.marginLeft  = "190px";
			document.getElementById('footerContent').style.marginLeft = "150px";
     } else if(document.getElementById('login')){
			document.getElementById('appArea').style.marginLeft  = "150px";
			document.getElementById('footerContent').style.marginLeft = "150px";
     } 
     //start code change for HT54219 by vasu on 18-Oct-05
     else if(document.getElementById('confirmDelete')){
     		document.getElementById('footerContent').style.marginLeft = "0px";
     }
     //end code change for HT54219 by vasu on 18-Oct-05
     
     else{
			document.getElementById('appArea').style.marginLeft  = "100px";
			document.getElementById('footerContent').style.marginLeft = "150px";
     }
  }

// Start Code Change by Renga for GNATS 13814 on 24-Sep-2004
// newWindow function opens window with required info.
  function newPopupWindow(url, anotherWin) {
      if (location.protocol != 'https:') {
          window.open("http://" + document.location.host + url, 'newWindow1', 'width=550,height=500,toolbar=no,status=no,menubar=no,scrollbars=yes,resizable=yes');
      } else {
          window.open("https://" + document.location.host + url, 'newWindow1', 'width=550,height=500,toolbar=no,status=no,menubar=no,scrollbars=yes,resizable=yes');
      }
	  anotherWindow.focus()
  }
// End Code Change by Renga for GNATS 13814 on 24-Sep-2004

// Start code added for GNATS 4800 by Pradeepa on 17-May-2004
 function setItUpNetscape(){
		if (! (typeof(NOMARGIN) == "undefined")) {
		document.getElementById('appArea').style.marginLeft = "0px";
		document.getElementById('footerContent').style.marginLeft = "100px";
		return;
		}

		if(document.getElementById('leftNavLook') ){
				document.getElementById('appArea').style.marginLeft = "100px";
				document.getElementById('footerContent').style.marginLeft = "150px";
		}
		else if( document.getElementById('siNavLook')){

					document.getElementById('appArea').style.marginLeft = "150px";
					document.getElementById('footerContent').style.marginLeft = "150px";
		}
		else if(document.getElementById('siPatternNavLook')){
				document.getElementById('appArea').style.marginLeft  = "195px";
				document.getElementById('footerContent').style.marginLeft = "150px";
		} else if(document.getElementById('login')){
				document.getElementById('appArea').style.marginLeft  = "150px";
				document.getElementById('footerContent').style.marginLeft = "150px";
		}
     //start code change for HT54219 by vasu on 18-Oct-05
     else if(document.getElementById('confirmDelete')){
     		document.getElementById('footerContent').style.marginLeft = "0px";
     }
     //end code change for HT54219 by vasu on 18-Oct-05
		else{
				document.getElementById('appArea').style.marginLeft  = "100px";
				document.getElementById('footerContent').style.marginLeft = "150px";
		}
}

// End code added for GNATS 4800 by Pradeepa on 17-May-2004

</script>

<script language="JavaScript" type="text/javascript">
//Start code changed for GNATS 4800 by Pradeepa on 17-May-2004

if(navigator.appName == "Netscape"){
	setItUpNetscape();
}
else{
	setItUp();
}
// End code changed for GNATS 4800 by Pradeepa on 17-May-2004
</script>


